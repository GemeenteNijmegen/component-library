{
    "name": "nijmegen-component-library",
    "version": "6.3.2",
    "description": "Nijmegen Component Library built by Enrise",
    "main": "",
    "repository": "https://github.com/Enrise/nijmegen-component-library",
    "license": "UNLICENSED",
    "browserslist": [
        "> .1% in NL"
    ],
    "postcss": {
        "plugins": {
            "autoprefixer": true
        }
    },
    "scripts": {
        "start": "NODE_ENV=development node server.js",
        "build": "NODE_ENV=production node server.js",
        "build-nlds": "rollup -c ./src/nlds/rollup.config.mjs",
        "test": "node_modules/.bin/jest",
        "lint-js": "eslint src/js/**/*.js",
        "lint-sass": "node node_modules/sass-lint/bin/sass-lint.js -v",
        "fix-sass": "node node_modules/sass-lint-fix/bin/sass-lint.js --fix",
        "create-release": "node helpers/createRelease.js"
    },
    "staticFiles": {
        "staticPath": [
            {
                "staticPath": "static"
            }
        ],
        "watcherGlob": "**"
    },
    "dependencies": {
        "@frctl/fractal": "^1.3.0",
        "@frctl/mandelbrot": "^1.3.0",
        "@mdi/font": "^5.8.55",
        "compare-versions": "^3.5.1",
        "del": "^3.0.0",
        "focus-trap": "^6.6.1",
        "js-yaml": "^3.12.0",
        "picker.js": "^0.1.11",
        "popper.js": "^1.16.0",
        "promise-polyfill": "^8.1.3",
        "through2": "^2.0.3",
        "tippy.js": "^6.3.7",
        "url-polyfill": "^1.1.12",
        "whatwg-fetch": "^3.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.10.5",
        "@babel/preset-env": "^7.10.4",
        "@gemeentenijmegen/design-tokens": "^0.0.2-alpha.0",
        "@rollup/plugin-node-resolve": "^15.3.0",
        "@utrecht/components": "^3.0.1-alpha.27",
        "@utrecht/focus-ring-css": "^2.3.0",
        "autoprefixer": "^9.7.1",
        "babel-jest": "^26.1.0",
        "eslint": "^5.4.0",
        "fs": "^0.0.1-security",
        "glob": "^7.1.4",
        "html-validator": "^4.1.1",
        "jest": "^24.9.0",
        "jquery": "^3.5.1",
        "jshint": "^2.10.2",
        "parcel-bundler": "^1.12.4",
        "parcel-plugin-static-files-copy": "^2.3.1",
        "postcss-discard-duplicates": "^7.0.1",
        "prettier": "^1.18.2",
        "regenerator-runtime": "^0.13.5",
        "rollup": "^4.28.1",
        "rollup-plugin-css-only": "^4.5.2",
        "rollup-plugin-postcss": "^4.0.2",
        "sass": "^1.23.3",
        "sass-lint": "^1.13.1",
        "sass-lint-fix": "^1.12.1",
        "ssri": "^8.0.0"
    }
}
