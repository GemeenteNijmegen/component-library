<script>
    var autocomplete = new Autocomplete();

    $(document).ready(function () {
        autocomplete.init(getAutocompleteResults)
    })

    function getAutocompleteResults(query, callback) {
        var rawResults = [
            { header: 'Zoekresultaten:' },
            { title: 'Finibus<em>' + query + '</em>', url: '?q=' + query },
            { title: '<em>' + query + '</em>elementum urna', url: '?q=' + query },
            { header: 'Zoeksuggesties:' },
            { title: 'Blandit aliquet<em>' + query + '</em>', url: '?q=' + query, class: 'search' },
            { title: query, url: '?q=' + query, class: 'search' }
        ];

        var results = rawResults
            .slice(0, Math.ceil(Math.random() * 6));
        if (results[results.length - 1].header) {
            results.pop();
        }
        callback(results);
    }
</script>
